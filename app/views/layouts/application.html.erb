<!DOCTYPE html>
<html>
  <head>
    <title>QuoraProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link href='https://fonts.googleapis.com/css?family=Abel' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="https://kit.fontawesome.com/d1f67cf0b7.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="//cdn.quilljs.com/1.3.1/quill.snow.css">
    <script src="//cdn.quilljs.com/1.3.6/quill.js"></script>
    <script type="text/javascript">window.userImg = "<%= image_url('user.png') %>";</script>
    <script type="text/javascript">window.logo = "<%= image_url('quora2.png') %>";</script>
    <%= favicon_link_tag asset_path('favicon.ico') %>




  </head>

  <body>
    <%= yield %>
  </body>
</html>
